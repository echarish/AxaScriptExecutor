<div class="container-fluid">

    <div class="row">
        <div class="col-lg-12">
            <div class="col-md-8 col-md-offset-2" style="margin-bottom:20px">
                <h2 class="alert alert-info blackiconcolor">Registered Users</h2>
                <hr class="colorgraph">
                <table id="registeredUsersTb" class="table table-striped table-bordered  blackiconcolor" cellspacing="0" width="100%">
                    <thead>
                    <tr class="tableHeader">
                        <th>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
                        <th>Username</th>
                        <th>Email</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% for(var i = 0; i < registeredusers.length; i++) { %>
                    <tr>
                        <td>
                            <a href="javascript:deleteUser('<%= registeredusers[i]._id %>');"><i
                                        class="fa fa-fw fa-trash  rediconcolor"></i>
                        </td>
                        <td><%= registeredusers[i].username %></td>
                        <td><%= registeredusers[i].email %></td>
                        <td><%= registeredusers[i].first_name %></td>
                        <td><%= registeredusers[i].last_name %></td>

                    </tr>
                    <% } %>
                    </tbody>
                </table>
            </div>
        </div>


    </div>
</div>
<script>
    $('#registeredUsersTb').DataTable();
</script>
