<div class="container-fluid">

    <div class="row">
        <div class="col-lg-12">
            <div class="col-md-8 col-md-offset-2  blackiconcolor" style="margin-bottom:20px">
                <h2 class="alert alert-info">Execution Log on All Servers</h2>
                <hr class="colorgraph">
                <table id="executionLogOnServerTb" class="table table-striped table-bordered " cellspacing="0" width="100%">
                    <thead>
                    <tr class="tableHeader">
                        <th>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
                        <th>Server Name</th>
                        <th>IP Address</th>
                        <th>Executed By</th>
                        <th>Execution Date</th>
                        <th>Script Name</th>
                        <th>Result</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% for(var i = 0; i < executionlog.length; i++) { %>
                    <tr>
                        <td>

                        </td>
                        <td><%= executionlog[i].server_name %></td>
                        <td><%= executionlog[i].ip_address %></td>
                        <td><%= executionlog[i].executed_by %></td>
                        <td><%= executionlog[i].executed_date %></td>
                        <td><%= executionlog[i].script_name %></td>
                        <td><%= executionlog[i].execution_result %></td>

                    </tr>
                    <% } %>
                    </tbody>
                </table>
            </div>
        </div>


    </div>
</div>
<script>
    $('#executionLogOnServerTb').DataTable();
</script>